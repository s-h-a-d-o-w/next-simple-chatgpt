{
  "name": "next-simple-chatgpt",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "build": "rimraf .next && tsx ./scripts/generateBuildTimestamp && next build",
    "create-certificate": "pnpm tsx ./scripts/createCertificate",
    "dev": "tsx ./scripts/checkWsl && rimraf .next && concurrently \"tsx ./scripts/runNext\" \"pnpm panda:watch\"",
    "lint": "next lint --ignore-path .gitignore",
    "panda:watch": "panda --watch",
    "precommit": "concurrently \"pnpm lint\" \"pnpm typecheck\"",
    "prepare": "panda codegen",
    "start": "next start",
    "start:local": "concurrently \"next start -p 3001\" \"npx local-ssl-proxy --key ./certificates/localhost-key.pem --cert ./certificates/localhost.pem --source 3000 --target 3001\"",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.54",
    "ai": "^3.3.19",
    "concurrently": "^8.2.2",
    "date-fns": "^3.6.0",
    "lodash": "^4.17.21",
    "next": "14.2.10",
    "next-auth": "5.0.0-beta.21",
    "prism-react-renderer": "^2.3.1",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.3.0",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "superjson": "^2.2.1",
    "use-local-storage-state": "^19.4.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.13",
    "@pandacss/dev": "^0.46",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.5",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.1",
    "typescript": "^5",
    "vitest": "^2.1.2"
  }
}
