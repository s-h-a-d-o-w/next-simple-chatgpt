{
  "name": "next-simple-chatgpt",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "beforebuild": "pnpm generate:refractor-languages && node ./scripts/generateBuildTimestamp.ts",
    "build": "pnpm beforebuild && next build",
    "build:e2e": "pnpm beforebuild && cross-env NEXT_PUBLIC_E2E=true next build",
    "create-certificate": "node ./scripts/createCertificate.ts",
    "predev": "pnpm generate:refractor-languages",
    "dev": "node ./scripts/checkWsl.ts && del-cli .next && concurrently \"node ./scripts/runNext.ts\" \"pnpm panda:watch\"",
    "dev:e2e": "node ./scripts/checkWsl.ts && del-cli .next && concurrently \"node ./scripts/runNext.ts --e2e\" \"pnpm panda:watch\"",
    "generate:refractor-languages": "node ./scripts/generateRefractorLanguageImports.ts",
    "lint": "eslint --cache --cache-strategy content",
    "panda:watch": "panda --watch",
    "precommit": "concurrently \"pnpm lint\" \"pnpm typecheck\"",
    "prepare": "concurrently \"husky\" \"panda codegen\"",
    "start": "next start",
    "start:local": "concurrently \"next start -p 3031\" \"npx local-ssl-proxy --key ./certificates/localhost-key.pem --cert ./certificates/localhost.pem --source 3030 --target 3031\"",
    "test": "vitest",
    "e2e": "cross-env NODE_ENV=production NEXT_PUBLIC_E2E=true  playwright test",
    "e2e:debug": "playwright test --debug",
    "e2e:ui": "playwright test --ui",
    "typecheck": "tsgo",
    "playground": "ts ./playground.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.44",
    "@ai-sdk/openai": "^2.0.67",
    "@ai-sdk/react": "^2.0.93",
    "@openrouter/ai-sdk-provider": "^1.2.3",
    "@pandacss/dev": "^0.53.7",
    "ai": "^5.0.93",
    "concurrently": "^8.2.2",
    "date-fns": "^3.6.0",
    "hast-util-to-jsx-runtime": "^2.3.6",
    "lodash": "^4.17.21",
    "next": "^16.0.3",
    "next-auth": "5.0.0-beta.30",
    "pdfjs-dist": "5.2.133",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "refractor": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "superjson": "^2.2.5",
    "use-local-storage-state": "^19.5.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@next/bundle-analyzer": "^16.0.3",
    "@next/eslint-plugin-next": "^16.0.3",
    "@playwright/test": "^1.56.1",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.19.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@typescript/native-preview": "7.0.0-dev.20251015.1",
    "cross-env": "^7.0.3",
    "del-cli": "^6.0.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.6.5+sha512.cdf928fca20832cd59ec53826492b7dc25dc524d4370b6b4adbf65803d32efaa6c1c88147c0ae4e8d579a6c9eec715757b50d4fa35eea179d868eada4ed043af",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  }
}
