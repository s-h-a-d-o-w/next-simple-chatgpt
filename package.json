{
  "name": "next-simple-chatgpt",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "prebuild": "pnpm generate:refractor-languages",
    "build": "tsx ./scripts/generateBuildTimestamp && next build",
    "create-certificate": "pnpm tsx ./scripts/createCertificate",
    "predev": "pnpm generate:refractor-languages",
    "dev": "tsx ./scripts/checkWsl && del-cli .next && concurrently \"tsx ./scripts/runNext\" \"pnpm panda:watch\"",
    "dev:e2e": "tsx ./scripts/checkWsl && del-cli .next && concurrently \"tsx ./scripts/runNext --e2e\" \"pnpm panda:watch\"",
    "generate:refractor-languages": "tsx ./scripts/generateRefractorLanguageImports.ts",
    "lint": "next lint --ignore-path .gitignore",
    "panda:watch": "panda --watch",
    "precommit": "concurrently \"pnpm lint\" \"pnpm typecheck\"",
    "prepare": "concurrently \"husky\" \"panda codegen\"",
    "start": "next start",
    "start:local": "concurrently \"next start -p 3001\" \"npx local-ssl-proxy --key ./certificates/localhost-key.pem --cert ./certificates/localhost.pem --source 3000 --target 3001\"",
    "test": "vitest",
    "e2e": "cross-env NODE_ENV=production playwright test",
    "e2e:debug": "playwright test --debug",
    "e2e:ui": "playwright test --ui",
    "typecheck": "tsc",
    "playground": "ts ./playground.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.0",
    "@ai-sdk/openai": "^2.0.0",
    "@ai-sdk/react": "^2.0.0",
    "ai": "^5.0.0",
    "concurrently": "^8.2.2",
    "date-fns": "^3.6.0",
    "hast-util-to-jsx-runtime": "^2.3.6",
    "lodash": "^4.17.21",
    "next": "^15.4.5",
    "next-auth": "5.0.0-beta.28",
    "pdfjs-dist": "5.2.133",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "refractor": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "superjson": "^2.2.2",
    "use-local-storage-state": "^19.5.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.5",
    "@pandacss/dev": "^0.53.7",
    "@playwright/test": "^1.54.2",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.17.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "cross-env": "^7.0.3",
    "del-cli": "^6.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.4.5",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.6.5+sha512.cdf928fca20832cd59ec53826492b7dc25dc524d4370b6b4adbf65803d32efaa6c1c88147c0ae4e8d579a6c9eec715757b50d4fa35eea179d868eada4ed043af",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  }
}
