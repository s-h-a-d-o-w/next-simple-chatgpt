{
  "name": "next-simple-chatgpt",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "build": "tsx ./scripts/generateBuildTimestamp && next build",
    "create-certificate": "pnpm tsx ./scripts/createCertificate",
    "dev": "tsx ./scripts/checkWsl && del-cli .next && concurrently \"tsx ./scripts/runNext\" \"pnpm panda:watch\"",
    "lint": "next lint --ignore-path .gitignore",
    "panda:watch": "panda --watch",
    "precommit": "concurrently \"pnpm lint\" \"pnpm typecheck\"",
    "prepare": "concurrently \"husky\" \"panda codegen\"",
    "start": "next start",
    "start:local": "concurrently \"next start -p 3001\" \"npx local-ssl-proxy --key ./certificates/localhost-key.pem --cert ./certificates/localhost.pem --source 3000 --target 3001\"",
    "test": "vitest",
    "e2e": "playwright test --ui",
    "typecheck": "tsc",
    "playground": "ts ./playground.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@ai-sdk/react": "^1.2.12",
    "ai": "^4.3.16",
    "concurrently": "^8.2.2",
    "date-fns": "^3.6.0",
    "lodash": "^4.17.21",
    "next": "^15.3.2",
    "next-auth": "5.0.0-beta.28",
    "pdfjs-dist": "^5.2.133",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1",
    "superjson": "^2.2.2",
    "use-local-storage-state": "^19.5.0",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.1.8",
    "@pandacss/dev": "^0.53.7",
    "@playwright/test": "^1.52.0",
    "@types/lodash": "^4.17.17",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "cross-env": "^7.0.3",
    "del-cli": "^6.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.1.8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.4.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "packageManager": "pnpm@10.6.5+sha512.cdf928fca20832cd59ec53826492b7dc25dc524d4370b6b4adbf65803d32efaa6c1c88147c0ae4e8d579a6c9eec715757b50d4fa35eea179d868eada4ed043af",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  }
}
